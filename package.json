    "@tanstack/react-query": "^4.35.3",
    "@tanstack/react-query-devtools": "^4.35.3",
    "@types/node": "20.2.5",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "axios": "^1.4.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "emailjs-com": "^3.2.0",
    "encoding": "^0.1.13",
